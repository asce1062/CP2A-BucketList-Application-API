# .coveragerc to control coverage.py
[run]
branch = False
# ignore imported modules and classes that will otherwise lower coverage.
omit = *clint*, *ctypes* , *sqlalchemy*, *decimal*, *numbers*, *databases*, *sqlite3*, *asyncio*, *concurrent*, *click*, *flask*, *jinja2*, *passlib*, *itsdangerous*, *markupsafe*, *werkzeug*, *crypt*, *hmac*, *html*, *mimetypes*, *multiprocessing*, *pydoc*, *socketserver*, *stringprep*, *uuid*, *psycopg2*, *http*, *encodings*, *aniso8601*, *dateutil*, *pytz*, *app/models.py*, *instance/__init__.py*